<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By" />
                <remove name="Access-Control-Allow-Origin" />
                <add name="Access-Control-Allow-Origin" value="https://zenmoney.ru" />
                <add name="Access-Control-Allow-Credentials" value="true" />
                <add name="Access-Control-Allow-Private-Network" value="true" />
                <add name="X-Powered-By" value="ASP.NET1" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
